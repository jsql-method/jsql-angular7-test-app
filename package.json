{
  "name": "jsql-angular7-test-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "concurrently --kill-others \"ng serve\" \"npm run jsql-watch\"",
    "jsql-watch": "jsql-cli --debug --apiKey=dawid.senko@jsql.it --input=/src/app/**/*.* --extensions=ts --devKeyFileName=test-key.key --env=local --watch",
    "jsql-build": "jsql-cli --debug --apiKey=dawid.senko@jsql.it --output=/dist/js/app.js --input=/dist/js/app.js --devKeyFileName=test-key.key --env=test",
    "build": "ng build --prod && npm run jsql-build",
    "ng-serve": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "jsql-angular7": "latest",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
